<div class="booyah-box col-10 offset-1">
  <h1>Edit Your Place Details</h1>   

  <%= simple_form_for @place do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :description %>
    <br />
    <%= f.submit 'Update', class: 'btn-primary' %>
 
    <div class="float-right">
        <%= link_to "<< Back <<", root_url %>
    </div>
    
  <% end %>
</div>
